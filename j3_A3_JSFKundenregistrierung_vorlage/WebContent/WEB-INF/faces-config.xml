<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<display-name>Chooser.xhtml</display-name>
		<from-view-id>/Chooser.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{kundeBean.choose}</from-action>
			<from-outcome>2</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Chooser.xhtml</display-name>
		<from-view-id>/Chooser.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{kundeBean.choose}</from-action>
			<from-outcome>1</from-outcome>
			<to-view-id>/FormKunde.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
